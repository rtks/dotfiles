{
  "title": "MouseGestures",
  "rules": [
    {
      "description": "Button 4 to shift for horizontal scroll and blender",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button4",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [
            { "key_code": "right_shift" }
          ],
          "to_if_alone": [
            { "pointing_button": "button4" }
          ]
        }
      ]
    },
    {
      "description": "Button 5 + left,right to desktop switch, middle to button 10, dpi to launchpad",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "apple_vendor_keyboard_key_code": "mission_control",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "up_arrow",
              "modifiers": "fn"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "button5_down",
              "value": 0
            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.universalcontrol"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "apple_vendor_keyboard_key_code": "mission_control",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "h",
              "modifiers": "fn"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "button5_down",
              "value": 1
            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.universalcontrol"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "pointing_button": "button1",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": "fn"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "button5_down",
              "value": 1
            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.universalcontrol"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "pointing_button": "button2",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": "fn"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "button5_down",
              "value": 1
            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.universalcontrol"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "pointing_button": "button5",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [
            { "set_variable": { "name": "button5_down", "value": 1 } }
          ],
          "to_after_key_up": [
            { "set_variable": { "name": "button5_down", "value": 0 } }
          ],
          "to_if_alone": [
            { "pointing_button": "button5" }
          ]
        },
        {
          "type": "basic",
          "from": {
            "pointing_button": "button1",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [
            { "key_code": "left_arrow", "modifiers": "left_control" }
          ],
          "conditions": [
            { "type": "variable_if", "name": "button5_down", "value": 1 }
          ]
        },
        {
          "type": "basic",
          "from": {
            "pointing_button": "button2",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [
            { "key_code": "right_arrow", "modifiers": "left_control" }
          ],
          "conditions": [
            { "type": "variable_if", "name": "button5_down", "value": 1 }
          ]
        },
        {
          "type": "basic",
          "from": {
            "pointing_button": "button3",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [
            { "pointing_button": "button10" }
          ],
          "conditions": [
            { "type": "variable_if", "name": "button5_down", "value": 1 }
          ]
        },
        {
          "type": "basic",
          "from": {
            "pointing_button": "button6",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [
            { "pointing_button": "button11" }
          ],
          "conditions": [
            { "type": "variable_if", "name": "button5_down", "value": 1 }
          ]
        },
        {
          "type": "basic",
          "from": {
            "pointing_button": "button7",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [
            { "pointing_button": "button12" }
          ],
          "conditions": [
            { "type": "variable_if", "name": "button5_down", "value": 1 }
          ]
        },
        {
          "type": "basic",
          "from": {
            "apple_vendor_keyboard_key_code": "mission_control",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [
            { "apple_vendor_keyboard_key_code": "launchpad" }
          ],
          "conditions": [
            { "type": "variable_if", "name": "button5_down", "value": 1 }
          ]
        }
      ]
    }
  ]
}
