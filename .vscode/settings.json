{
    "workbench.activityBar.visible": false,
    "workbench.startupEditor": "newUntitledFile",
    "editor.formatOnPaste": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "workbench.statusBar.feedback.visible": false,
    "files.associations": {
        "vimrc": "viml"
    },
    "workbench.colorTheme": "Solarized Dark",
    "editor.fontFamily": "'SFMonoSquareVSCodeHack', \"SF Mono Square\"",
    "editor.fontSize": 13,
    "editor.lineHeight": 13,
    "editor.fontLigatures": true,
    "terminal.integrated.shell.osx": "/usr/local/bin/fish",
    "terminal.integrated.shell.windows": "C:\\Ubuntu18.04\\ubuntu1804.exe",
    "commands.commands" : [
        {
            "text": "$(three-bars)",
            "command": "workbench.action.toggleSidebarVisibility",
            "tooltip": "サイドバーの表示切替え",
            "priority": 20000
        },
        {
            "text": "$(file-directory)",
            "command": "workbench.view.explorer",
            "tooltip": "エクスプローラー",
            "priority": 20000
        },
        {
            "text": "$(search)",
            "command": "workbench.view.search",
            "tooltip": "検索",
            "priority": 20000
        },
        {
            "text": "$(checklist)",
            "command": "workbench.view.extension.todo",
            "tooltip": "TODO+",
            "priority": 20000
        },
        {
            "text": "$(repo-forked)",
            "command": "workbench.view.scm",
            "tooltip": "ソース管理",
            "priority": 20000
        },
        {
            "text": "$(repo)",
            "command": "workbench.view.extension.gitlens",
            "tooltip": "GitLens",
            "priority": 20000
        },
        {
            "text": "$(bug)",
            "command": "workbench.view.debug",
            "tooltip": "デバッグ",
            "priority": 20000
        }
    ],
    "terminal.integrated.copyOnSelection": true,
    "workbench.editor.tabCloseButton": "left"
}