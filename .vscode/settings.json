{
    "workbench.activityBar.visible": false,
    "workbench.startupEditor": "newUntitledFile",
    "editor.formatOnPaste": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "workbench.statusBar.feedback.visible": false,
    "files.associations": {
        "vimrc": "viml",
        "*.omnijs": "javascript"
    },
    "workbench.colorTheme": "Solarized Light",
    "editor.fontSize": 13,
    "editor.fontLigatures": true,
    "terminal.integrated.shell.osx": "/opt/homebrew/bin/fish",
    "terminal.integrated.shell.windows": "C:\\Ubuntu18.04\\ubuntu1804.exe",
    "commands.commands" : [
        {
            "text": "$(three-bars)",
            "command": "workbench.action.toggleSidebarVisibility",
            "tooltip": "サイドバーの表示切替え",
            "priority": 20000
        },
        {
            "text": "$(file-directory)",
            "command": "workbench.view.explorer",
            "tooltip": "エクスプローラー",
            "priority": 20000
        },
        {
            "text": "$(search)",
            "command": "workbench.view.search",
            "tooltip": "検索",
            "priority": 20000
        },
        {
            "text": "$(checklist)",
            "command": "workbench.view.extension.todo",
            "tooltip": "TODO+",
            "priority": 20000
        },
        {
            "text": "$(repo-forked)",
            "command": "workbench.view.scm",
            "tooltip": "ソース管理",
            "priority": 20000
        },
        {
            "text": "$(repo)",
            "command": "workbench.view.extension.gitlens",
            "tooltip": "GitLens",
            "priority": 20000
        },
        {
            "text": "$(bug)",
            "command": "workbench.view.debug",
            "tooltip": "デバッグ",
            "priority": 20000
        }
    ],
    "terminal.integrated.copyOnSelection": true,
    "workbench.editor.tabCloseButton": "left",
    "rust-client.rustupPath": "/Users/ryota/.cargo/bin/rustup",
    "window.zoomLevel": 0,
    "editor.fontFamily": "'SF Mono 1x2','SF Mono Square',monospace",
    "editor.lineHeight": 13,
    "explorer.confirmDelete": false,
    "remote.SSH.useLocalServer": false,
    "remote.SSH.remotePlatform": {
        "pi": "linux",
        "takasu.dev": "linux"
    },
    "workbench.preferredDarkColorTheme": "Solarized Dark",
    "workbench.preferredLightColorTheme": "Solarized Light",
    "window.autoDetectColorScheme": true
}